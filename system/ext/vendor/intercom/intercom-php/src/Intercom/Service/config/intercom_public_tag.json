{
    "name": "Intercom API (Tag)",
    "description": "The Intercom API (Tag)",
    "operations": {
        "getTags": {
            "extends": "_abstract_pagination_page",
            "httpMethod": "GET",
            "responseClass": "TagListModel",
            "responseType": "model",
            "summary": "Get a list of tags",
            "uri": "/tags"
        },
        "createTag": {
            "httpMethod": "POST",
            "parameters": {
                "name": {
                    "location": "json",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "TagModel",
            "responseType": "model",
            "summary": "Creates a tag",
            "uri": "/tags"
        },
        "updateTag": {
            "httpMethod": "POST",
            "parameters": {
                "id": {
                    "location": "json",
                    "required": true,
                    "type": "string"
                },
                "name": {
                    "location": "json",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "TagModel",
            "responseType": "model",
            "summary": "Updates a tag",
            "uri": "/tags"
        },
        "tagUsers": {
            "httpMethod": "POST",
            "parameters": {
                "name": {
                    "location": "json",
                    "required": true,
                    "type": "string"
                },
                "users": {
                    "location": "json",
                    "required": true,
                    "type": "array",
                    "items": {
                        "id": {
                            "location": "json",
                            "required": false,
                            "type": "string"
                        },
                        "email": {
                            "location": "json",
                            "required": false,
                            "type": "string"
                        },
                        "user_id": {
                            "location": "json",
                            "required": false,
                            "type": "string"
                        },
                        "untag": {
                            "location": "json",
                            "required": false,
                            "type": "boolean"
                        }
                    }
                }
            },
            "responseClass": "TagModel",
            "responseType": "model",
            "summary": "Tags or untags users",
            "uri": "/tags"
        },
        "tagCompanies": {
            "httpMethod": "POST",
            "parameters": {
                "name": {
                    "location": "json",
                    "required": true,
                    "type": "string"
                },
                "companies": {
                    "location": "json",
                    "required": true,
                    "type": "array",
                    "items": {
                        "id": {
                            "location": "json",
                            "required": false,
                            "type": "string"
                        },
                        "company_id": {
                            "location": "json",
                            "required": false,
                            "type": "string"
                        },
                        "untag": {
                            "location": "json",
                            "required": false,
                            "type": "boolean"
                        }
                    }
                }
            },
            "responseClass": "TagModel",
            "responseType": "model",
            "summary": "Tags or untags companies",
            "uri": "/tags"
        }
    },
    "models": {
        "TagModel": {
            "type": "object",
            "properties": {
                "id": {
                    "location": "json",
                    "type": "string"
                },
                "name": {
                    "location": "json",
                    "type": "string"
                },
                "x_rate_limit": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Limit",
                    "type": "integer"
                },
                "x_rate_remaining": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Remaining",
                    "type": "integer"
                },
                "x_rate_reset": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Reset",
                    "type": "integer"
                }
            }
        },
        "TagListModel": {
            "type": "object",
            "properties": {
                "tags": {
                    "location": "json",
                    "type": "array",
                    "items": {
                        "$ref": "TagModel"
                    }
                },
                "x_rate_limit": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Limit",
                    "type": "integer"
                },
                "x_rate_remaining": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Remaining",
                    "type": "integer"
                },
                "x_rate_reset": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Reset",
                    "type": "integer"
                }
            }
        }
    }
}
