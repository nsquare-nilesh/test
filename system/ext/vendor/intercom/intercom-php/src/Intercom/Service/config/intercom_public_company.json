{
    "name": "Intercom API (Company)",
    "description": "The Intercom API (Company)",
    "operations": {
        "getCompanies": {
            "extends": "_abstract_pagination_page",
            "httpMethod": "GET",
            "parameters": {
                "tag_id": {
                    "location": "query",
                    "required": false,
                    "type": "string"
                },
                "segment_id": {
                    "location": "query",
                    "required": false,
                    "type": "string"
                }
            },
            "responseClass": "CompanyListModel",
            "responseType": "model",
            "summary": "Gets a list of companies",
            "uri": "/companies"
        },
        "getCompany": {
            "httpMethod": "GET",
            "parameters": {
                "id": {
                    "location": "uri",
                    "required": false,
                    "type": "string"
                },
                "name": {
                    "location": "query",
                    "required": false,
                    "type": "string"
                },
                "company_id": {
                    "location": "query",
                    "required": false,
                    "type": "string"
                }
            },
            "responseClass": "CompanyModel",
            "responseType": "model",
            "summary": "Gets a single company",
            "uri": "/companies/{id}"
        },
        "getCompanyUsers": {
            "extends": "_abstract_pagination_page",
            "httpMethod": "GET",
            "parameters": {
                "id": {
                    "location": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "responseClass": "UserListModel",
            "responseType": "model",
            "summary": "Gets all company users",
            "uri": "/companies/{id}/users"
        },
        "getCompanyUsersByCompanyID": {
            "extends": "_abstract_pagination_page",
            "httpMethod": "GET",
            "parameters": {
                "company_id": {
                    "location": "query",
                    "required": true,
                    "type": "string"
                },
                "type": {
                    "location": "query",
                    "required": true,
                    "type": "string",
                    "default": "user"
                }
            },
            "responseClass": "UserListModel",
            "responseType": "model",
            "summary": "Gets all company users",
            "uri": "/companies"
        },
        "createCompany": {
            "httpMethod": "POST",
            "parameters": {
                "remote_created_at": {
                    "location": "json",
                    "required": false,
                    "type": "integer",
                    "format": "timestamp"
                },
                "company_id": {
                    "location": "json",
                    "required": true,
                    "type": "string"
                },
                "name": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "monthly_spend": {
                    "location": "json",
                    "required": false,
                    "type": "integer"
                },
                "plan": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "session_count": {
                    "location": "json",
                    "required": false,
                    "type": "integer"
                },
                "custom_attributes": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                }
            },
            "responseClass": "CompanyModel",
            "responseType": "model",
            "summary": "Creates a new company",
            "uri": "/companies"
        },
        "updateCompany": {
            "httpMethod": "POST",
            "parameters": {
                "remote_created_at": {
                    "location": "json",
                    "required": false,
                    "type": "integer",
                    "format": "timestamp"
                },
                "company_id": {
                    "location": "json",
                    "required": true,
                    "type": "string"
                },
                "name": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "monthly_spend": {
                    "location": "json",
                    "required": false,
                    "type": "number"
                },
                "plan": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "session_count": {
                    "location": "json",
                    "required": false,
                    "type": "integer"
                },
                "custom_attributes": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                }
            },
            "responseClass": "CompanyModel",
            "responseType": "model",
            "summary": "Updates the Company",
            "uri": "/companies"
        }
    },
    "models": {
        "CompanyModel": {
            "type": "object",
            "properties": {
                "app_id": {
                    "location": "json",
                    "type": "string"
                },
                "created_at": {
                    "location": "json",
                    "type": "integer"
                },
                "custom_attributes": {
                    "location": "json",
                    "type": "object"
                },
                "id": {
                    "location": "json",
                    "type": "string"
                },
                "last_request_at": {
                    "location": "json",
                    "type": "integer",
                    "format": "timestamp"
                },
                "monthly_spend": {
                    "location": "json",
                    "type": "number"
                },
                "name": {
                    "location": "json",
                    "type": "string"
                },
                "plan": {
                    "type": "object",
                    "items": {
                        "type": {
                            "location": "json",
                            "type": "string"
                        },
                        "id": {
                            "location": "json",
                            "type": "string"
                        },
                        "name": {
                            "location": "json",
                            "type": "string"
                        }
                    }
                },
                "remote_created_at": {
                    "location": "json",
                    "type": "integer",
                    "format": "timestamp"
                },
                "session_count": {
                    "location": "json",
                    "type": "integer"
                },
                "segments": {
                    "location": "json",
                    "type": "object",
                    "properties": {
                        "segment.list": {
                            "type": "array",
                            "location": "json",
                            "items": {
                                "$ref": "SegmentModel"
                            }
                        }
                    }
                },
                "tags": {
                    "location": "json",
                    "type": "object",
                    "properties": {
                        "tag.list": {
                            "type": "array",
                            "location": "json",
                            "items": {
                                "$ref": "TagModel"
                            }
                        }
                    }
                },
                "updated_at": {
                    "location": "json",
                    "type": "integer",
                    "format": "timestamp"
                },
                "user_count": {
                    "location": "json",
                    "type": "integer"
                },
                "x_rate_limit": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Limit",
                    "type": "integer"
                },
                "x_rate_remaining": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Remaining",
                    "type": "integer"
                },
                "x_rate_reset": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Reset",
                    "type": "integer"
                }
            }
        },
        "CompanyListModel": {
            "type": "object",
            "properties": {
                "companies": {
                    "location": "json",
                    "type": "array",
                    "items": {
                        "$ref": "CompanyModel"
                    }
                },
                "x_rate_limit": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Limit",
                    "type": "integer"
                },
                "x_rate_remaining": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Remaining",
                    "type": "integer"
                },
                "x_rate_reset": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Reset",
                    "type": "integer"
                }
            }
        }
    }
}
